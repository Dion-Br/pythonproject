{
    "commands": [
        {
            "name": "Get IP",
            "command": "ipconfig"
        },
        {
            "name": "System info",
            "command": "systeminfo"
        },
        {
            "name": "Local network info",
            "command": "arp -a"
        },
        {
            "name": "All wifi passwords",
            "command": 'for /f "skip=9 tokens=1,2 delims=:" %i in ("netsh wlan show profiles") do @echo %j | findstr -i -v echo | netsh wlan show profiles %j key=clear'
        }
    ]
}
